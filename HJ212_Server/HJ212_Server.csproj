<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageId>CSoft.Protocol.HJ212_Server</PackageId>
    <Version>1.0.4</Version>
    <Authors>CSoft</Authors>
    <Description>HJ212服务端</Description>
    <PackageIcon>logo.png</PackageIcon>
    <RepositoryUrl>https://github.com/caoyiliang/HJ212_Server</RepositoryUrl>
    <PackageProjectUrl>https://github.com/caoyiliang/HJ212_Server</PackageProjectUrl>
    <PackageReleaseNotes>修复C26、C27、C28 bug</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\logo.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="CSoft.Protocol.ProtocolInterface" Version="8.2.4" />
  </ItemGroup>

</Project>
